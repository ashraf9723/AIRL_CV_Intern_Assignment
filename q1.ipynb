Â  Â  Â  Â  "Â  Â  Â  Â  return x\n",
Â  Â  Â  Â  "Â  Â  keep_prob = 1 - drop_prob\n",
Â  Â  Â  Â  "Â  Â  shape = (x.shape[0],) + (1,) * (x.ndim - 1)Â  # work with diff dim tensors, not just 2D ConvNets\n",
Â  Â  Â  Â  "Â  Â  random_tensor = keep_prob + torch.rand(shape, dtype=x.dtype, device=x.device)\n",
Â  Â  Â  Â  "Â  Â  random_tensor.floor_()Â  # binarize\n",
Â  Â  Â  Â  "Â  Â  output = x.div(keep_prob) * random_tensor\n",
Â  Â  Â  Â  "Â  Â  return output"
